{
  "name": "mirror-display",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec 'npm run client:dev && npm run server:dev' --ext js,json --watch src/server",
    "server:stop": "pkill --signal SIGINT server-mirror-display || true",
    "server:clean": "rm -rf dist/server",
    "server:build": "webpack --progress --colors --config src/server/webpack.config.js",
    "server:start": "node dist/server/server.bundle.js server-mirror-display",
    "server:dev": "npm run server:stop && npm run server:clean && npm run server:build && npm run server:start",
    "client:clean": "rm -rf dist/client",
    "client:build": "mkdir -p dist/client && cp src/client/index.html dist/client && webpack --progress --colors --config src/client/webpack.config.js",
    "client:dev": "npm run client:clean && npm run client:build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.1",
    "engine.io": "^1.6.11",
    "engine.io-client": "^1.6.11",
    "express": "^4.14.0",
    "forecast.io-bluebird": "0.0.1",
    "lodash": "^4.14.1",
    "moment": "2.14.1",
    "node-schedule": "^1.1.1",
    "node-uuid": "^1.4.7",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "^4.4.5",
    "redux": "3.5.2",
    "rethinkdb": "^2.3.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.9.0",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.11.1",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "json-loader": "^0.5.4",
    "node-sass": "3.8.0",
    "nodemon": "^1.10.0",
    "npm-watch": "^0.1.5",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-livereload-plugin": "0.8.1"
  }
}
